<algorithm id="mesh_reader" name="input">
  <parameter name="filename" type="string">../data/mixed_mesh.vtu</parameter>
</algorithm>

<algorithm id="simplexify" name="simplexify">
  <default_source>input</default_source>
</algorithm>

<algorithm id="mesh_writer" name="output">
  <default_source>simplexify</default_source>
  <parameter name="filename" type="string">simplexified.vtu</parameter>
</algorithm>
