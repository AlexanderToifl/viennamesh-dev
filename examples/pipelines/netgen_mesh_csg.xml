<algorithm type="netgen_csg_mesh_generator" name="mesher">
  <parameter type="string" name="default">
    algebraic3d
    solid cube = orthobrick (0, 0, 0; 1, 10, 1);
    tlo cube;
  </parameter>
</algorithm>

<algorithm type="mesh_writer" name="output">
  <default_source>mesher</default_source>
  <parameter name="filename" type="string">blub.vtu</parameter>
</algorithm>

